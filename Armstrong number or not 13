import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int a = in.nextInt();
        
        if(a>=100 && 1000>a)
            System.out.println("Yes");
        else
            System.out.println("No");
    }
}
