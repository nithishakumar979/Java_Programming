import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int a = in.nextInt();
        
        if(a>0)
            System.out.println("positive");
        else if(a<0)
            System.out.println("negative");
        else
            System.out.println("zero");
        
            
    }
}
